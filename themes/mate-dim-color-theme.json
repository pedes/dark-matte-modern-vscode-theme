    "editorWarning.foreground": "#D2AF74",
    "editorWarning.border": "#D2AF7455",
    "editorInfo.foreground": "#7EA8D0",
    "editorInfo.border": "#7EA8D055",
    "editorHint.foreground": "#78A891",
    "editorHint.border": "#78A89155",
    "editorIndentGuide.background1": "#171B23",
    "editorIndentGuide.activeBackground1": "#313B4E",
    "editorBracketMatch.background": "#2A33434D",
    "editorBracketMatch.border": "#55627B",
    "editorBracketHighlight.foreground1": "#6EA2D8",
    "editorBracketHighlight.foreground2": "#83BAA0",
    "editorBracketHighlight.foreground3": "#C7AA79",
    "editorBracketHighlight.foreground4": "#B79AD8",
    "editorBracketHighlight.foreground5": "#D38E7F",
    "editorBracketHighlight.foreground6": "#8EA4CF",
    "editorBracketPairGuide.background1": "#2A334433",
    "editorBracketPairGuide.background2": "#2F3A4D33",
    "editorBracketPairGuide.background3": "#34415833",
    "editorBracketPairGuide.background4": "#3A486233",
    "editorBracketPairGuide.background5": "#41506D33",
    "editorBracketPairGuide.background6": "#48587833",
    "editorBracketPairGuide.activeBackground1": "#6EA2D866",
    "editorBracketPairGuide.activeBackground2": "#83BAA066",
    "editorBracketPairGuide.activeBackground3": "#C7AA7966",
    "editorBracketPairGuide.activeBackground4": "#B79AD866",
    "editorBracketPairGuide.activeBackground5": "#D38E7F66",
    "editorBracketPairGuide.activeBackground6": "#8EA4CF66",
    "editorWhitespace.foreground": "#263041",
    "editorRuler.foreground": "#1A2230",
    "editorCodeLens.foreground": "#5E697D",
    "editorInlayHint.foreground": "#7E8AA0",
    "editorInlayHint.background": "#121A2399",
    "editorInlayHint.parameterForeground": "#8A9BB5",
    "editorInlayHint.parameterBackground": "#15223499",
    "editorInlayHint.typeForeground": "#8CB0D4",
    "editorInlayHint.typeBackground": "#1A203299",
    "editorGutter.addedBackground": "#6F9C83",
    "editorGutter.deletedBackground": "#C06C7E",
    "editorGutter.modifiedBackground": "#95A9D4",

    "activityBar.background": "#0B0D11",
    "activityBar.foreground": "#C8CDD7",
    "activityBar.inactiveForeground": "#6A7280",
    "activityBarBadge.background": "#5F77B1",
    "activityBarBadge.foreground": "#090B0F",

    "sideBar.background": "#0C0F14",
    "sideBar.foreground": "#AEB5C2",
    "sideBar.border": "#181D27",
    "sideBarTitle.foreground": "#C0C7D3",
    "sideBarSectionHeader.background": "#0B0E13",
    "sideBarSectionHeader.foreground": "#959EAD",
    "list.activeSelectionBackground": "#1A212D",
    "list.activeSelectionForeground": "#D2D8E2",
    "list.inactiveSelectionBackground": "#141B25",
    "list.hoverBackground": "#161D28",
    "list.highlightForeground": "#AAB7D3",

    "titleBar.activeBackground": "#0B0D11",
    "titleBar.activeForeground": "#C4CBD7",
    "titleBar.inactiveBackground": "#0B0D11CC",
    "titleBar.inactiveForeground": "#7C8697",

    "statusBar.background": "#0A0C10",
    "statusBar.foreground": "#AEB5C2",
    "statusBar.debuggingBackground": "#4D5E7E",
    "statusBar.debuggingForeground": "#E3E8F1",
    "statusBar.noFolderBackground": "#0A0C10",
    "statusBarItem.hoverBackground": "#1B2230",

    "panel.background": "#0A0D12",
    "panel.border": "#1A202B",
    "panelTitle.activeForeground": "#CBD2DE",
    "panelTitle.inactiveForeground": "#788293",
    "textLink.foreground": "#7EB0DA",
    "textLink.activeForeground": "#9CC3E8",

    "tab.activeBackground": "#11161E",
    "tab.activeForeground": "#CAD1DD",
    "tab.inactiveBackground": "#0B0D11",
    "tab.inactiveForeground": "#727B8C",
    "tab.border": "#171D27",

    "input.background": "#11161E",
    "input.foreground": "#C3CAD7",
    "input.border": "#283142",
    "input.placeholderForeground": "#666F80",
    "inputOption.activeBorder": "#6279B4",

    "dropdown.background": "#11161E",
    "dropdown.foreground": "#C3CAD7",
    "dropdown.border": "#283142",

    "scrollbarSlider.background": "#2A334499",
    "scrollbarSlider.hoverBackground": "#3B475D99",
    "scrollbarSlider.activeBackground": "#4F5F7999",

    "minimap.background": "#07090C",
    "minimap.findMatchHighlight": "#586784",
    "minimap.selectionHighlight": "#2A3344",
    "minimap.errorHighlight": "#C97888CC",
    "minimap.warningHighlight": "#D2AF74CC",
    "minimapGutter.addedBackground": "#78A891CC",
    "minimapGutter.deletedBackground": "#C97888CC",
    "minimapGutter.modifiedBackground": "#95A9D4CC",

    "peekViewEditor.background": "#080B10",
    "peekViewEditor.matchHighlightBackground": "#46537366",
    "peekViewResult.background": "#0C1016",
    "peekViewResult.matchHighlightBackground": "#4653734D",
    "peekView.border": "#263041",

    "terminal.background": "#07090C",
    "terminal.foreground": "#C5CBD8",
    "terminal.ansiBlack": "#07090C",
    "terminal.ansiRed": "#C06C7E",
    "terminal.ansiGreen": "#7FA98C",
    "terminal.ansiYellow": "#C0A670",
    "terminal.ansiBlue": "#7D91BA",
    "terminal.ansiMagenta": "#A287D6",
    "terminal.ansiCyan": "#7FA2C4",
    "terminal.ansiWhite": "#CDD3DF",
    "terminal.ansiBrightBlack": "#545D6D",
    "terminal.ansiBrightRed": "#CF7E90",
    "terminal.ansiBrightGreen": "#94BCA1",
    "terminal.ansiBrightYellow": "#CCB882",
    "terminal.ansiBrightBlue": "#92A6CE",
    "terminal.ansiBrightMagenta": "#B197E2",
    "terminal.ansiBrightCyan": "#95B7D8",
    "terminal.ansiBrightWhite": "#E0E5EE",

    "diffEditor.insertedTextBackground": "#27443466",
    "diffEditor.removedTextBackground": "#4B293266",
    "diffEditor.insertedLineBackground": "#1D312855",
    "diffEditor.removedLineBackground": "#3D242B55",
    "diffEditor.diagonalFill": "#10151D",

    "gitDecoration.addedResourceForeground": "#7FAC95",
    "gitDecoration.modifiedResourceForeground": "#95A9D4",
    "gitDecoration.deletedResourceForeground": "#C97888",
    "gitDecoration.untrackedResourceForeground": "#7FAC95",
    "gitDecoration.ignoredResourceForeground": "#5E6777",
    "gitDecoration.conflictingResourceForeground": "#D2AF74"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#667083",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.link.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "entity.name.section.markdown",
        "markup.heading.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#6FAEEA"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#D1AF7E"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#8EA4CF"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#7FCB9E"
      }
    },
    {
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#BBC3D2"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#9DB0D0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#E09B8A"
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "string.regexp",
        "constant.other.character-class.regexp"
      ],
      "settings": {
        "foreground": "#B9CBE8"
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "variable.other.property",
        "support.type.property-name",
        "entity.other.attribute-name",
        "entity.name.tag",
        "meta.tag",
        "meta.tag entity"
      ],
      "settings": {
        "foreground": "#8CB7DA"
      }
    },
    {
      "scope": [
        "string.quoted",
        "meta.tag string.quoted",
        "meta.attribute-with-value string.quoted",
        "entity.other.attribute-name.xml"
      ],
      "settings": {
        "foreground": "#D1AF7E"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "meta.tag constant"
      ],
      "settings": {
        "foreground": "#E09B8A"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#A7B2C7"
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "meta.namespace"
      ],
      "settings": {
        "foreground": "#8FB9D9"
      }
    },
    {
      "scope": [
        "entity.name.enum",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#D9BA87"
      }
    },
    {
      "scope": [
        "entity.name.type.parameter",
        "entity.name.type.template"
      ],
      "settings": {
        "foreground": "#A9BFE0"
      }
    },
    {
      "scope": [
        "meta.annotation",
        "storage.type.annotation",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#B49BD9"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#0A0C10",
        "background": "#C06C7E"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#B7C4DD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#A2ACBC",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#88A0CB"
      }
    },
    {
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#C06C7E"
      }
    },
    {
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#A58DD1"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "comment": "#667083",
    "variable": "#C0C8D7",
    "variable.readonly": "#AEB8CB",
    "variable.constant": "#E2BC87",
    "parameter": "#9FB4CD",
    "property": "#88B4D8",
    "namespace": "#8FB9D9",
    "class": "#9FB2D4",
    "interface": "#A5B5DA",
    "enum": "#C3AEDB",
    "enumMember": "#E0BD8A",
    "typeParameter": "#A9BFE0",
    "function": "#7FCB9E",
    "function.declaration": "#88D2A5",
    "method": "#7FCB9E",
    "function.defaultLibrary": "#7BC596",
    "macro": "#B9A2DC",
    "decorator": "#B49BD9",
    "type": "#9FB2D4",
    "type.defaultLibrary": "#9FB2D4",
    "string": "#D1AF7E",
    "regexp": "#B9CBE8",
    "number": "#DE9C8D",
    "operator": "#A7B2C7",
    "keyword": "#8EA4CF",
    "label": "#82A5C8"
  }
}
